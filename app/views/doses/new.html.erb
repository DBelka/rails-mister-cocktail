<h1>New Dose</h1>
<div class="container">
  <div class="row justify-center-center">
    <div class="col-md-8">

    <%= simple_form_for [@cocktail, @dose] do |f| %>
      <%= f.input :description %>
      <%= f.association :ingredient %>

      <%= f.button :submit, class: "btn btn-primary" %>
    <% end %>
    </div>
  </div>
</div>
