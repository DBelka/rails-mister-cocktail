  <div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(https://images.unsplash.com/photo-1523529364348-e930dcb39642?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1050&q=80" alt="Card image cap);">
  <div class="background">
    <h1><strong>All Cocktails</strong></h1>
    <p>Get a load of these thirst quenchers!</p>
  </div>
</div>
  <br>

  <div class='container'>
    <div class="cards">
      <% @cocktails.each do |cocktail| %>
        <div class="card" style="width: 18rem;">

          <img class="card-img-top" src="https://images.unsplash.com/photo-1523529364348-e930dcb39642?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1050&q=80" alt="Card image cap">
          <div class="card-body text-center">
            <h5 class="card-title"><%= cocktail.name %></h5>
            <%= link_to 'Inspect', cocktail_path(cocktail), class: 'btn btn-primary' %>
          </div>
        </div>
      <% end %>
    </div>
    <br>
    <button class='btn btn-primary text-center'><%= link_to "Add Cocktail", new_cocktail_path %></button>
  </div>
